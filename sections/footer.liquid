              <button type="submit" disabled name="commit" id="Subscribe" class="btn btn-primary">
                <span>{{ 'general.newsletter_form.submit' | t }}</span>
              </button>
            </div>
          {%  else %}
            <input type="email"
                   name="contact[email]"
                   id="Email"
                   value=""
                   placeholder="{{ 'general.newsletter_form.email_placeholder' | t }}"
                   autocorrect="off"
                   autocapitalize="off"
                   data-parsley-type="email"
                   data-parsley-required="true"
                   data-parsley-errors-messages-enabled>
            <div class="cta">
              <button type="submit" name="commit" id="Subscribe" class="btn btn-primary">
                <span>{{ 'general.newsletter_form.submit' | t }}</span>
